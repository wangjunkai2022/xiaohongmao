<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/secondaryProgress">
        <shape android:shape="ring" android:useLevel="true" android:innerRadius="18.0dip" android:thickness="2.0dip">
            <solid android:color="@color/md_grey_100" />
        </shape>
    </item>
    <item android:id="@android:id/progress">
        <rotate android:fromDegrees="270.0" android:toDegrees="270.0" android:pivotX="50.0%" android:pivotY="50.0%">
            <shape android:shape="ring" android:useLevel="true" android:innerRadius="18.0dip" android:thickness="2.0dip">
                <solid android:color="@color/imAccent" />
            </shape>
        </rotate>
    </item>
</layer-list>