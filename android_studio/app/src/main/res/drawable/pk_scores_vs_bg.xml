<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient android:startColor="#ff7db5fd" android:endColor="#ffffaed7" android:angle="0" android:type="linear"/>
        </shape>
    </item>
    <item android:left="-25dp" android:right="-25dp">
        <shape android:shape="rectangle">
            <corners android:topLeftRadius="3dp" android:bottomLeftRadius="3dp"/>
            <stroke android:width="2dp" android:color="#fadd75"/>
        </shape>
    </item>
</layer-list>
